msgid ""
"\n"
" \n"
" \n"
"                                   XFE, X File Explorer File Manager\n"
" \n"
"                                     <http://roland65.free.fr/xfe>\n"
"                                 <http://sourceforge.net/projects/xfe>\n"
" \n"
" \n"
" \n"
